{
  "name": "template",
  "version": "0.0.0",
  "scripts": {
    "devui": "vite --config vite.config.web.js",
    "dev-cr": "npm run clear-cr && npm run prepare-cr && npm run build-cr",
    "build-cr": "cross-env VENDOR=chromium NODE_ENV=development vite build",
    "build-cr-cs": "vite build --config vite.config.content.js",
    "clear-cr": "rimraf extension/chromium/dist extension/chromium/manifest.json extension.*",
    "prepare-cr": "cross-env VENDOR=chromium NODE_ENV=development node ./src/createManifest.js",
    "gen-icons": "node ./src/icons/generateTooltipIcons.js",
    "build-web": "vite build --config vite.config.web.js",
    "serve-web": "vite preview --config vite.config.web.js"
  },
  "dependencies": {
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "cross-env": "^7.0.3",
    "formik": "^2.2.9",
    "gsap": "^3.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-responsive": "^9.0.0-beta.6",
    "react-router-dom": "^5.3.0",
    "react-select": "^5.2.2",
    "react-sortablejs": "^6.1.0",
    "react-switch": "^6.0.0",
    "rgb-hex": "^4.0.0",
    "rimraf": "^3.0.2",
    "short-uuid": "^4.2.0",
    "styled-components": "^5.3.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.0",
    "canvas": "^2.9.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "svg-fill": "^0.3.7",
    "tailwindcss": "^2.2.15",
    "twin.macro": "^2.7.0",
    "vite": "^2.6.4",
    "vite-plugin-babel-macros": "^1.0.5"
  }
}