{
  "manifest_version": 2,
  "name": "Opener",
  "version": "1.0.1",
  "description": "Open Multiple Websites With The Same Query",
  "icons": {
    "16": "./icons/icon-512.png",
    "48": "./icons/icon-512.png"
  },
  "content_scripts": [
    {
      "matches" :  ["*://*/*"],
      "js": [
        "content-scripts.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "./icons/icon-512.png",
      "38": "./icons/icon-512.png"
    },
    "default_title": "Google Translate Theme",
    "default_popup": "./dist/popup/index.html"
  },
  "permissions": [
    "*://*/*",
    "tabs",
    "storage",
    "activeTab",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": [
    "themes.js"
  ]
}